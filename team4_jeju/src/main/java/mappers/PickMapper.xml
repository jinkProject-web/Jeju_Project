<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="user">

<insert id="inputPick" parameterType="pick">
	INSET INTO PICK(PICK_MNUM,PLACE_NUM)
	VALUES(PICK_MNUM_SEQ.NEXTVAL,#{Place_Num})
</insert>


</mapper>